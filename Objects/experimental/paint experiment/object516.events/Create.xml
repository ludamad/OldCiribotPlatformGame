      </arguments>
    </action>
    <action id="102" library="1">
      <!--action name: Set_Motion-->
      <kind>NORMAL</kind>
      <allowRelative>true</allowRelative>
      <question>false</question>
      <canApplyTo>true</canApplyTo>
      <actionType>FUNCTION</actionType>
      <functionName>action_set_motion</functionName>
      <relative>false</relative>
      <not>false</not>
      <appliesTo>.self</appliesTo>
      <arguments>
        <argument kind="EXPRESSION">random(360)</argument>
        <argument kind="EXPRESSION">floor(random(11))+1</argument>
      </arguments>
    </action>
  </actions>
